{
  "productName": "calendar-app",
  "version": "1.0.0",
  "identifier": "com.calendar-app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../out"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Calendar App",
        "width": 1000
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": ["appimage", "nsis"],
        "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {},
    "sql": {}
  }
}